<!DOCTYPE html>
<html lang="en">
    <%- include('./partials/head.ejs') %>
<body>
    <h2> Transaction Table </h2>
    <br>
    <br>
    <div class="table-wrapper">
        <table class="fl-table">
            <thead>
                <tr>
                    <th>Transaction ID</th>
                    <th>Staff ID</th>
                    <th>Date</th>
                    <th>Milk Type</th>
                    <th>Quantity</th>
                    <th>Rate</th>
                    <th>Total</th>
                    <th>Type</th>
                </tr>
            </thead>
            <tbody>
                <% for(var i=0; i < rows.length; i++) { %>
                    <tr>
                        <td><%= rows[i].transaction_id %></td>
                        <td><%= rows[i].staff_id %></td>
                        <td><%= rows[i].date %></td>
                        <td><%= rows[i].milk_type %></td>
                        <td><%= rows[i].quantity %></td>
                        <td><%= rows[i].rate %></td>
                        <td><%= rows[i].total %></td>
                        <td><%= rows[i].type %></td>
                   </tr>
                <% } %>

            </tbody>
            
        </table>

    </div>

    <br>

    <footer> 
        <a href="/">Home</a>
    </footer>
    
</body>
</html>