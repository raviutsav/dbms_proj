<!DOCTYPE html>
<html lang="en">
    <%- include('./partials/head.ejs') %>
<body>
    <h2>Stock Table</h2>
    <br>
    <br>
    <div class="table-wrapper">
        <table class="fl-table">
            <thead>
                <tr>
                    <th>Milk Type</th>
                    <th>Quantity</th>
                </tr>

            </thead>
            <tbody>

                <% for(var i=0; i < rows.length; i++) { %>
                    <tr>
                        <td><%= rows[i].milk_type %></td>
                        <td><%= rows[i].quantity %></td>
                   </tr>
                <% } %>
            </tbody>
            
        </table>

    </div>
    

    <br>
    
    <footer> 
        <a href="/">Home</a>
    </footer>
</body>
</html>